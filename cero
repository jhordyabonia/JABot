::objetives
[var base number 10]
::objetives
::knowledge
[cerovar]
::knowledge
::test
[calculate r = x base]
[if r]
[stop]
[/if]
::test
::logic
[calculate test > x base]
[if test]
[calculate z - x y]
[else]
[calculate z + x y]
[/if]
[calculate test > z x]
[if test]
[make cerovar]
[var x number var::z]
[var y number var::y]
[/make]
[break]
[else]
[calculate z + y y]
[/if]
[calculate test > z x]
[if test]
[make cerovar]
[var x number var::z]
[var y number var::y]
[/make]
[break]
[else]
[calculate z - y y]
[/if]
[calculate test > z x]
[if test]
[make cerovar]
[var x number var::z]
[var y number var::y]
[/make]
[break]
[/if]
[cero]
::logic